{
  "name": "",
  "version": "0.0.0",
  "license": "ISC",
  "scripts": {
    "prepare": "husky install",
    "commit": "cz",
    "start": "npm run generate-svg && ng serve",
    "generate-svg": "node src/assets/svg/src/generate-svg.js",
    "lint": "eslint {**/*,*}.{js,ts,html}",
    "lint:fix": "eslint {**/*,*}.{js,ts,html} --fix",
    "prettier": "prettier {**/*,*}.{html,js,ts,md,scss,css}",
    "prettier:fix": "prettier {**/*,*}.{html,js,ts,md,scss,css} --write",
    "stylelint": "stylelint {**/*,*}.{scss,css}",
    "stylelint:fix": "stylelint {**/*,*}.{scss,css} --fix",
    "test": "jest -c jest.config.ts --silent",
    "test:watch": "npm run test -- --watch",
    "test:cc": "npm run test -- --coverage",
    "test:cc:since-develop": "npm run test:cc -- --changedSince origin/develop",
    "test:cc:since-main": "npm run test:cc -- --changedSince origin/main",
    "build": "npm run generate-svg && ng build client-portal",
    "build:dev": "npm run build -- --configuration dev",
    "build:test": "npm run build -- --configuration test",
    "build:integration": "npm run build -- --configuration int",
    "build:prod": "npm run build -- --configuration production",
    "build:local:stats": "npm run build -- --stats-json",
    "build:dev:stats": "npm run build -- --configuration dev --stats-json",
    "build:test:stats": "npm run build -- --configuration test --stats-json",
    "build:integration:stats": "npm run build -- --configuration int --stats-json",
    "build:prod:stats": "npm run build -- --configuration production --stats-json",
    "analyze": "webpack-bundle-analyzer dist/client-portal/stats.json",
    "analyze:sme": "source-map-explorer dist/client-portal/**/*.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.1.7",
    "@angular/common": "16.1.7",
    "@angular/compiler": "16.1.7",
    "@angular/core": "16.1.7",
    "@angular/forms": "16.1.7",
    "@angular/material": "16.1.6",
    "@angular/platform-browser": "16.1.7",
    "@angular/platform-browser-dynamic": "16.1.7",
    "@angular/router": "16.1.7",
    "@ngneat/input-mask": "6.1.0",
    "@types/inputmask": "5.0.3",
    "dayjs": "1.11.9",
    "rfc6902": "5.0.1",
    "rfdc": "1.3.0",
    "rxjs": "7.8.1",
    "tslib": "2.5.3",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.6",
    "@angular-devkit/core": "16.1.6",
    "@angular-devkit/schematics": "16.1.6",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "16.1.6",
    "@angular/compiler-cli": "16.1.7",
    "@angular/language-service": "16.1.7",
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@commitlint/cz-commitlint": "17.6.7",
    "@schematics/angular": "16.1.6",
    "@types/ejs": "3.1.2",
    "@types/jest": "29.5.3",
    "@types/node": "18.16.0",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "commitizen": "4.3.0",
    "ejs": "3.1.10",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-rxjs": "5.0.3",
    "glob": "10.3.3",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "jest-preset-angular": "13.1.1",
    "lint-staged": "13.2.3",
    "modern-css-reset": "1.4.0",
    "ng-mocks": "14.11.0",
    "normalize.css": "8.0.1",
    "prettier": "3.0.0",
    "source-map-explorer": "2.5.3",
    "stylelint": "15.10.2",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-standard-scss": "10.0.0",
    "svgo": "3.0.2",
    "svgstore-cli": "2.0.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "webpack-bundle-analyzer": "4.9.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "engines": {
    "node": "18.16.0",
    "npm": "9.5.1"
  }
}
