{
  "name": "ng-template",
  "version": "0.0.0",
  "license": "ISC",
  "scripts": {
    "prepare": "husky install",
    "commit": "cz",
    "start": "npm run generate-svg && ng serve",
    "generate-svg": "node src/assets/svg/src/generate-svg.js",
    "lint": "eslint {**/*,*}.{js,ts,html}",
    "lint:fix": "eslint {**/*,*}.{js,ts,html} --fix",
    "prettier": "prettier {**/*,*}.{html,js,ts,md,scss,css}",
    "prettier:fix": "prettier {**/*,*}.{html,js,ts,md,scss,css} --write",
    "stylelint": "stylelint {**/*,*}.{scss,css}",
    "stylelint:fix": "stylelint {**/*,*}.{scss,css} --fix",
    "test": "jest -c jest.config.ts --silent",
    "test:watch": "npm run test -- --watch",
    "test:cc": "npm run test -- --coverage",
    "test:cc:since-develop": "npm run test:cc -- --changedSince origin/develop",
    "test:cc:since-main": "npm run test:cc -- --changedSince origin/main",
    "build": "npm run generate-svg && ng build ng-template",
    "build:dev": "npm run build -- --configuration dev",
    "build:test": "npm run build -- --configuration test",
    "build:integration": "npm run build -- --configuration int",
    "build:prod": "npm run build -- --configuration production",
    "build:local:stats": "npm run build -- --stats-json",
    "build:dev:stats": "npm run build -- --configuration dev --stats-json",
    "build:test:stats": "npm run build -- --configuration test --stats-json",
    "build:integration:stats": "npm run build -- --configuration int --stats-json",
    "build:prod:stats": "npm run build -- --configuration production --stats-json",
    "analyze": "webpack-bundle-analyzer dist/ng-template/stats.json",
    "analyze:sme": "source-map-explorer dist/ng-template/**/*.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.8",
    "@angular/common": "17.3.8",
    "@angular/compiler": "17.3.8",
    "@angular/core": "17.3.8",
    "@angular/forms": "17.3.8",
    "@angular/material": "17.3.8",
    "@angular/platform-browser": "17.3.8",
    "@angular/platform-browser-dynamic": "17.3.8",
    "@angular/router": "17.3.8",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.7",
    "@angular-devkit/core": "17.3.7",
    "@angular-devkit/schematics": "17.3.7",
    "@angular-eslint/eslint-plugin": "17.4.0",
    "@angular-eslint/eslint-plugin-template": "17.4.0",
    "@angular-eslint/template-parser": "17.4.0",
    "@angular/cli": "17.3.7",
    "@angular/compiler-cli": "17.3.8",
    "@angular/language-service": "17.3.8",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@commitlint/cz-commitlint": "19.2.0",
    "@schematics/angular": "17.3.7",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.11",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "commitizen": "4.3.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-rxjs": "5.0.3",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-preset-angular": "14.0.4",
    "lint-staged": "15.2.2",
    "modern-css-reset": "1.4.0",
    "ng-mocks": "14.12.2",
    "normalize.css": "8.0.1",
    "prettier": "3.2.5",
    "source-map-explorer": "2.5.3",
    "stylelint": "16.5.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-standard-scss": "13.1.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "webpack-bundle-analyzer": "4.10.2"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "engines": {
    "node": "^20.9.0",
    "npm": "^10.0.0"
  }
}
